Source code examples from: http://zetcode.com/databases/mysqlphptutorial/

$ mysql -u root -p

mysql>CREATE DATABASE php_tutorial;
mysql>CREATE USER 'justin'@'localhost' IDENTIFIED BY '46656';
mysql>USE php-tutorial;
mysql>GRANT ALL ON php_tutorial.* to 'justin'@'localhost';
mysql>FLUSH PRIVILEGES;
mysql>quit

$ php version.php
$ php populate.php
$ php fetch_assoc.php
$ php fetch_row.php
$ php fetch_object.php
$ php escape_string.php
$ php fetch_field.php
$ php num_field_rows.php
